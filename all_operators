#include<stdio.h>
int main()
{
    int a,b,sum,difference,product,division,mod,greaterthan,lessthan,equal,notequal,logicalor,logicaland,logicalnot,bitwiseor,bitwiseand;
    printf("enter a and b");
    scanf("%d%d",&a,&b);
    sum=a+b;
    difference=a-b;
    product=a*b;
    division=a/b;
    mod=a%b;
    greaterthan=a>b;
    lessthan=a<b;
    equal=(a==b);
    notequal=(a!=b);
    logicalor=(a==b||a>b);
    logicaland=(a>b&&a<b);
    logicalnot=!(a!=b&&a>b);
    bitwiseor=(a<b|b>a);
    bitwiseand=(a=b&b<a);
    printf("sum=%d\ndifference=%d\nproduct=%d\ndivision=%d\nmod=%d\ngreaterthan=%d\nlessthan=%d\nequal=%d\nnotequal=%d\n",sum,difference,product,division,mod,greaterthan,lessthan,equal,notequal);
    printf("logicalor=%d\nlogicaland=%d\nlogicalnot=%d\n,bitwiseor=%d\nbitwiseand=%d",logicalor,logicaland,logicalnot,bitwiseor,bitwiseand);
    return 0;
}
