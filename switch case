#include<stdio.h>
int main()
{
    int choice,a,b,c;
    printf("enter the choice");
    scanf("%d",&choice);
    printf("enter a and b");
    scanf("%d%d",&a,&b);
    switch(choice)
    {
        case 1:c=a+b;
        printf("sum is %d",c);
        break;
        case 2:c=a-b;
        printf("difference is %d",c);
        break;
        case 3:c=a*b;
        printf("product id %d",c);
        break;
        case 4:c=a/b;
        printf("division is %d",c);
        break;
        case 5:c=a%b;
        printf("mod is %d",c);
        break;
        default:
        printf("enter the choice between 1 and 5");
    }
    return 0;
}
